<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gavin.superrefreshlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar"
        >
        <activity android:name=".MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RecyclerViewRefreshActivity" />
        <activity android:name=".ListViewRefreshActivity" />
        <activity android:name=".NestedScrollRefreshActivity" />
        <activity android:name=".ScrollViewRefreshActivity" />
        <activity android:name=".NormalViewRefreshActivity" />

    </application>

</manifest>